}

#free-trial {
	margin-left:600px;
	width:100px;
	height:20px;
	margin-top:-49px; 
}
	 
a.free-trial {
display: block;
width: 106px;
height: 29px;
background: url(/sites/all/themes/accellion/images/btn/btn-free-trial.png) no-repeat 0 0;
overflow: hidden;
text-indent: -15000px;
font-size: 0px;
line-height: 0px;
}
	 
a:hover.free-trial {
    background-position: 0 -29px;
}
 
a:active.free-trial {
    background-position: 0 -106px;
}







